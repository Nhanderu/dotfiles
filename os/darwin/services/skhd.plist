<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>homebrew.mxcl.skhd</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/opt/skhd/bin/skhd</string>
      <string>--config=/Users/rescobar/.config/skhd/config</string>
    </array>
    <key>RunAtLoad</key>
    <true />
    <key>KeepAlive</key>
    <true />
    <key>WorkingDirectory</key>
    <string>/Users/rescobar/.local/share/skhd</string>
    <key>StandardErrorPath</key>
    <string>/Users/rescobar/.local/share/skhd/stderr.log</string>
    <key>StandardOutPath</key>
    <string>/Users/rescobar/.local/share/skhd/stdout.log</string>
  </dict>
</plist>
