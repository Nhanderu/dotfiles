<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>homebrew.mxcl.redis</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/opt/redis/bin/redis-server</string>
      <string>/Users/rescobar/.config/redis/redis.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true />
    <key>KeepAlive</key>
    <true />
    <key>WorkingDirectory</key>
    <string>/Users/rescobar/.local/share/redis</string>
    <key>StandardErrorPath</key>
    <string>/Users/rescobar/.local/share/redis/stderr.log</string>
    <key>StandardOutPath</key>
    <string>/Users/rescobar/.local/share/redis/stdout.log</string>
  </dict>
</plist>
